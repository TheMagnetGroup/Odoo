<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="stock_pick_form_inherit_delivery_iot" model="ir.ui.view">
		<field name="name">stock_.pick.form.inherit.delivery_iot</field>
		<field name="model">stock.picking</field>
		<field name="inherit_id" ref="stock.view_picking_form"/>
		<field name="arch" type="xml">
			<xpath expr="//button[@name='action_toggle_is_locked']" position="after">
				<button name="action_print_labels"
					string="Print Shipping Labels"
					type="object"
					states="done"/>
			</xpath>
		</field>
	</record>
</odoo>