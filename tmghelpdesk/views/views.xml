<odoo>
  <data>

    <record id="tmg_helpdesk_ticket_view_kanban" model="ir.ui.view">
        <field name="name">tmg.helpdesk.ticket.kanban</field>
        <field name="model">helpdesk.ticket</field>
        <field name="priority">100</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_kanban"/>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
          <xpath expr="//field[@name='partner_id']" position="after">
            <field name="create_date"/>
          </xpath>
        </field>
    </record>

    <record id = "tmg_helpdesk_ticket_view_list" model="ir.ui.view">
      <field name="name">tmg.helpdesk.ticket.list</field>
      <field name="model">helpdesk.ticket</field>
      <field name="priority">100</field>
      <field name="inherit_id" ref="helpdesk.helpdesk_ticket_tree"/>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='partner_id']" position="after">
          <field name="create_date"/>
        </xpath>
      </field>
    </record>
    
  </data>
</odoo>