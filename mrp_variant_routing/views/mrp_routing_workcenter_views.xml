<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="mrp_routing_workcenter_tree_inherit_mrp_variant_routing" model="ir.ui.view">
		<field name="name">mrp.routing.workcenter.tree.inherit.mrp_variant_routing</field>
		<field name="model">mrp.routing.workcenter</field>
		<field name="inherit_id" ref="mrp.mrp_routing_workcenter_tree_view"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='workcenter_id']" position="after">
				<field name="attribute_value_ids" widget="many2many_tags"/>
			</xpath>
		</field>
	</record>
	<record id="mrp_routing_workcenter_form_inherit_mrp_variant_routing" model="ir.ui.view">
		<field name="name">mrp.routing.workcenter.form.inherit.mrp_variant_routing</field>
		<field name="model">mrp.routing.workcenter</field>
		<field name="inherit_id" ref="mrp.mrp_routing_workcenter_form_view"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='workcenter_id']" position="after">
				<field name="attribute_value_ids" widget="many2many_tags"/>
			</xpath>
		</field>
	</record>
</odoo>