<odoo>
    <data>
        <record id="tmg_attachment_types" model="ir.ui.view">
            <field name="name">tmg.attachment.types</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/sheet[1]/notebook[1]" position="inside">
                    <page name="attachment_tab" string="Attachments">
                        <field name="attachments" attrs="{}">
                            <tree class="o_documents_list_view" create="false" edit="false">
                                <field name="type" invisible="1"/>
                                <field name="thumbnail" options="{'size':[90,90]}" widget="image"/>
                                <field name="datas" widget="binary" filename="datas_fname"
                                       attrs="{'invisible':[('type','=','url')]}"/>
                                <field name="datas_fname" invisible="1" attrs="{'invisible':[('type','=','url')]}"
                                       class="oe_inline oe_right"/>
                                <field name="url" widget="binary" filename="datas_fname"
                                       attrs="{'invisible':[('type','=','binary')]}"/>
                                <field name="write_uid"/>
                                <field name="create_date"/>
                                <field name="attachment_category" string="Category" widget="many2many_tags"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
                <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='attachment_tab']/field[@name='attachments']/tree[1]"
                       position="after">
                    <form string="Attachments">
                        <sheet name="studio_sheet_06a203">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                            <group name="studio_group_cd7a11">
                                <group name="studio_group_c7e4f7">
                                    <field name="type"/>
                                    <field name="datas" filename="datas_fname"
                                           attrs="{'invisible':[('type','=','url')]}"/>
                                    <field name="datas_fname" invisible="1" attrs="{'invisible':[('type','=','url')]}"
                                           class="oe_inline oe_right"/>
                                    <field name="url" widget="url" attrs="{'invisible':[('type','=','binary')]}"/>
                                    <field name="mimetype" groups="base.group_no_one"/>
                                    <field name="attachment_category" widget="many2many_tags" string="Tags"/>
                                </group>
                                <group string="Attached To" groups="base.group_no_one" name="studio_group_627ca9">
                                    <field name="res_model"/>
                                    <field name="res_field"/>
                                    <field name="res_id"/>
                                    <field name="res_name"/>
                                    <field name="company_id" groups="base.group_multi_company"
                                           options="{'no_create': True}"/>
                                    <field name="public"/>
                                </group>
                                <group string="History" groups="base.group_no_one" name="studio_group_8e3948">
                                    <label for="create_uid" string="Creation"/>
                                    <div name="creation_div">
                                        <field name="create_uid" readonly="1" class="oe_inline"/>
                                        on
                                        <field name="create_date" readonly="1" class="oe_inline"/>
                                    </div>
                                </group>
                                <group name="description_group" string="Description" groups="base.group_no_one"
                                       colspan="4">
                                    <field name="description" nolabel="1"/>
                                </group>
                                <group groups="base.group_no_one" string="Indexed Content" colspan="4"
                                       name="studio_group_2595af">
                                    <field name="index_content" nolabel="1"/>
                                </group>
                            </group>
                        </sheet>
                    </form>
                </xpath>
            </field>
        </record>
    </data>
</odoo>

