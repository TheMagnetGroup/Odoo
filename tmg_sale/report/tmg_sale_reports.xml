<odoo>
    <data>
        <template id="tmg_sale_order_report_delivery" inherit_id="sale.report_saleorder_document">
            <!-- Add the delivery method and if 3rd party also show service level and account number -->
            <xpath expr="//div[@id='informations']" position="after">
                <div class="row mt32 mb32" id="delivery_informations">
                    <div t-if="doc.carrier_id" class="col-auto mw-100 mb-2">
                        <strong>Delivery Method:</strong>
                        <p class="m-0" t-field="doc.carrier_id"/>
                    </div>
                    <div t-if="doc.fedex_bill_my_account" class="col-auto mw-100 mb-2">
                        <strong>FedEx Carrier Account:</strong>
                        <p class="m-0" t-field="doc.fedex_carrier_account"/>
                    </div>
                    <div t-if="doc.fedex_bill_my_account" class="col-auto mw-100 mb-2">
                        <strong>FedEx Service Type:</strong>
                        <p class="m-0" t-field="doc.fedex_service_type"/>
                    </div>
                    <div t-if="doc.ups_bill_my_account" class="col-auto mw-100 mb-2">
                        <strong>Carrier Account:</strong>
                        <p class="m-0" t-field="doc.ups_carrier_account"/>
                    </div>
                    <div t-if="doc.ups_bill_my_account" class="col-auto mw-100 mb-2">
                        <strong>UPS Service Type:</strong>
                        <p class="m-0" t-field="doc.ups_service_type"/>
                    </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>