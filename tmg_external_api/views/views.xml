<record id="action_tmg_external_api" model="ir.actions.act_window">
            <field name="name">External API</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tmg_external_api.api_logging</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form,pivot</field>
        </record>

    <record id="action_tmg_external_api_promostandards" model="ir.actions.act_window">
            <field name="name">Promostandards</field>
